<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IndicatorRatingBar">
        <attr name="selectable" format="boolean" />
        <attr name="indicatorTitle" format="string" />
        <attr name="indicatorPosition" format="enum">
            <enum name="indicatorUp" value="0" />
            <enum name="indicatorDown" value="1" />
        </attr>
        <attr name="rateType" format="enum">
            <enum name="numeric" value="0" />
            <enum name="custom" value="1" />
        </attr>
        <attr name="rateShape" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="square" value="1" />
        </attr>
        <attr name="minRate" format="integer" />
        <attr name="maxRate" format="integer" />
        <attr name="selectedRate" format="integer" />
    </declare-styleable>

    <declare-styleable name="RatingCell">
        <attr name="layout_square" format="boolean" />
    </declare-styleable>
</resources>